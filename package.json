{
  "name": "planscaler-mindmap",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "netlify dev",
    "start": "npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "format": "prettier --write .",
    "db:migrate": "dbmate -u $DATABASE_URL up",
    "db:rollback": "dbmate -u $DATABASE_URL down 1",
    "deploy": "npm run build && netlify deploy --prod --dir=dist"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "axios": "^1.5.0",
    "clsx": "^2.0.0"
  },
  "devDependencies": {
    "vite": "^4.4.0",
    "@vitejs/plugin-react": "^4.0.0",
    "netlify-cli": "^17.12.0",
    "typescript": "^5.1.6",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.1",
    "prettier": "^2.8.8",
    "dbmate": "^1.25.0",
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=18"
  }
}