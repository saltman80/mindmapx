{
  "name": "netlify-vite-neon-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev:web": "vite",
    "dev:api": "netlify functions:serve --port=8888",
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:api\"",
    "compile": "tsc -p tsconfig.migrations.json",
    "compile:functions": "tsc -p tsconfig.functions.json",
    "build": "npm run compile && npm run compile:functions && npm run migrate && vite build",
    "preview": "vite preview",
    "migrate": "node dist/runmigrations.js",
    "start": "npm run dev"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "dotenv": "^16.0.3",
    "openai": "^4.5.0",
    "@netlify/functions": "^1.1.0",
    "uuid": "^8.3.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.10.0",
    "stripe": "^12.0.0",
    "zod": "^3.21.4",
    "@vercel/postgres": "^0.10.0"
  },
  "devDependencies": {
    "typescript": "^5.1.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.6.6",
    "@types/uuid": "^8.3.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/stripe": "^8.0.417",
    "@netlify/functions": "^1.0.0"
  }
}