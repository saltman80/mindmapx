{
  "name": "netlify-vite-neon-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev:web": "vite",
    "dev:api": "netlify functions:serve --port=8888",
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:api\"",
    "build": "npm run compile:migrations && npm run migrate && vite build",
    "preview": "vite preview",
    "compile:migrations": "tsc -p tsconfig.migrations.json",
    "migrate": "node dist/runmigrations.js",
    "start": "npm run dev"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "dotenv": "^16.0.3",
    "openai": "^4.5.0",
    "@netlify/functions": "^3.0.0",
    "zod": "^3.21.0",
    "@vercel/postgres": "^0.10.0",
    "pg": "^8.10.0"
  },
  "devDependencies": {
    "typescript": "^5.1.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.6.6"
  }
}