{
  "name": "netlify-vite-neon-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev:web": "vite",
    "dev:api": "netlify functions:serve --port=8888",
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:api\"",
    "build": "npx vite build",
    "preview": "vite preview",
    "compile:migrations": "npx tsc -p tsconfig.json",
    "migrate": "node dist/runmigrations.js",
    "start": "npm run dev"
  },
  "dependencies": {
    "@netlify/functions": "^3.0.0",
    "openai": "^4.5.0",
    "zod": "^3.21.0",
    "@vercel/postgres": "^1.0.0"
  },
  "devDependencies": {
    "typescript": "^5.1.0",
    "@types/node": "^20.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/bcrypt": "^5.0.2",
    "@types/uuid": "^9.0.8",
    "@types/pg": "^8.11.6",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "stripe": "^15.8.0",
    "@types/stripe-v3": "^3.1.33",
    "vite": "^5.2.11"
  }
}
